{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "server/__pycache__": true,
        "server/__init__.py": true,
        "server/tests/__pycache__": true
    },
    "hide-files.files": [
        "server/__pycache__",
        "server/__init__.py",
        "server/tests/__pycache__"
    ]
}